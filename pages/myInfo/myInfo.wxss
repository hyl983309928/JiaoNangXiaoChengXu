  color: #333;
}

.chart_bar{
  width:750rpx;
  margin-top: 40rpx;
}
.canvasBar{
  width:750rpx;
  height:460rpx;
}
.share{
  position: fixed;
  right: 20rpx;
  bottom: 20rpx;
  width:104rpx;
  height:104rpx;
  background: rgba(66,209,217,0.8);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.share image{
  width:59rpx;
  height:60rpx;
}

.action{
  color: #42d1d9 !important;
}

.borderBottom{
  width:100%;
  height:6rpx;
}
.borderBottom view{
  transition: 0.3s all;
  width:250rpx;
  height:6rpx;
  background: #42d1d9;
}

.list{
  width:100%;
  overflow: hidden;
}
.listBox{
  box-sizing: border-box;
  width:100%;
  white-space: nowrap;
  transition: 0.3s all;
  background: #fff;
}

.capsuleListBox{
  box-sizing: border-box;
  display: inline-block;
  background: #fff;
  width:750rpx;
  white-space: normal;
  overflow: hidden;
  vertical-align: top;
}

.capsuleItemBox{
  padding: 30rpx;
  background: #fff;
  vertical-align: top;
  box-sizing: border-box;
  /*margin-bottom: 20rpx;*/
}
.capsuleItemBox .content{
  padding-bottom: 28rpx;
  border-bottom: 1px solid #e5e5e5;
  position: relative;
}
.capsuleItemBox .cover{
  width:206rpx;
  height: 206rpx;
  margin-right: 32rpx;
  border-radius: 20rpx;
}
.capsuleItemBox .info{
  display: inline-block;
  vertical-align: top;
  width:450rpx;
}
.capsuleItemBox .title{
  font-size: 36rpx;
  font-weight: bold;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 22rpx;
  margin-top: 8rpx;
}
.capsuleItemBox .target{
  margin-bottom: 14rpx;
}
.capsuleItemBox .target text{
  display: inline-block;
  padding: 0 20rpx;
  height:36rpx;
  line-height: 36rpx;
  text-align: center;
  border-bottom-left-radius: 18rpx;
  border-top-right-radius: 18rpx;
  margin-right: 23rpx;
}
.capsuleItemBox .introduce{
  font-size: 26rpx;
  color: #666666;
  line-height: 40rpx;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
  overflow: hidden;
  text-overflow:ellipsis;
}
.capsuleItemBox .footInfo{
  font-size: 24rpx;
  color: #ccc;
  padding-top: 26rpx;
}
.capsuleItemBox .footInfo .time{
  display: inline-block;
  margin-right: 15rpx;
}
.capsuleItemBox .age{
  display: inline-block;
  float: right;
  color: #999;
}


