  margin-bottom: 50rpx;
}
.footitem{
  display: inline-flex;
  justify-content: center;
  align-items: center;
  margin-right: 47rpx;
}
.footitem:last-child{
  margin-right: 0;
}
.footitem .line{
  display: inline-block;
  width:64rpx;
  height:10rpx;
  border-radius: 5rpx;
  margin-right: 21rpx;
}
.footitem .label{
  display: inline-block;
